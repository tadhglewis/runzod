{
  "name": "runzod",
  "version": "2.0.0",
  "description": "Codemod to migrate from runtypes to zod",
  "main": "dist/index.js",
  "bin": {
    "runzod": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:codemod": "node dist/index.js __testfixtures__ --dry --verbose",
    "prepare": "pnpm build",
    "start": "node dist/index.js",
    "version": "changeset version",
    "changeset": "changeset",
    "release": "pnpm build && changeset publish",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "codemod",
    "migration",
    "runtypes",
    "zod",
    "typescript",
    "jscodeshift"
  ],
  "author": "Tadhg Lewis",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tadhglewis/runzod"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "jscodeshift": "17.3.0",
    "glob": "11.0.1"
  },
  "devDependencies": {
    "@changesets/cli": "2.28.1",
    "@types/jscodeshift": "17.3.0",
    "runtypes": "7.0.4",
    "typescript": "5.8.2",
    "vitest": "3.1.1",
    "zod": "3.24.2"
  }
}