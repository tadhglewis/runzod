{
  "name": "runzod",
  "version": "1.0.0",
  "description": "Codemod to migrate from runtypes to zod",
  "main": "dist/index.js",
  "bin": {
    "runzod": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:codemod": "jscodeshift -t src/transform.ts --dry --print --run-in-band __testfixtures__/basic.input.ts",
    "prepare": "npm run build",
    "start": "node dist/index.js"
  },
  "keywords": [
    "codemod",
    "migration",
    "runtypes",
    "zod",
    "typescript",
    "jscodeshift"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=12.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/jscodeshift": "^17.3.0",
    "jest": "^29.7.0",
    "jscodeshift": "^17.3.0",
    "runtypes": "^7.0.4",
    "ts-jest": "^29.3.1",
    "typescript": "^5.8.2",
    "zod": "^3.24.2"
  }
}
