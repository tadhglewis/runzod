{
  "name": "runzod",
  "version": "1.0.0",
  "description": "Codemod to migrate from runtypes to zod",
  "main": "dist/index.js",
  "bin": {
    "runzod": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:codemod": "jscodeshift -t src/transform.ts --parser=ts --dry --print --run-in-band __testfixtures__/basic.input.ts",
    "prepare": "npm run build",
    "start": "node dist/index.js"
  },
  "keywords": [
    "codemod",
    "migration",
    "runtypes",
    "zod",
    "typescript",
    "jscodeshift"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "jscodeshift": "^17.3.0"
  },
  "devDependencies": {
    "@types/jscodeshift": "^17.3.0",
    "runtypes": "^7.0.4",
    "typescript": "^5.8.2",
    "vitest": "^3.1.1",
    "zod": "^3.24.2"
  }
}
